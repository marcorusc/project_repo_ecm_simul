<PhysiCell_settings version="devel-version">
	<domain>
		<x_min>-400.0</x_min>
		<x_max>400.0</x_max>
		<y_min>-400.0</y_min>
		<y_max>400.0</y_max>
		<z_min>-20.0</z_min>
		<z_max>20.0</z_max>
		<dx>10.0</dx>
		<dy>10.0</dy>
		<dz>10.0</dz>
		<use_2D>false</use_2D>
	</domain>
	
	<overall>
		<max_time units="min">3300.0</max_time>
		<time_units>min</time_units>
		<space_units>micron</space_units>

		<dt_diffusion units="min">0.02</dt_diffusion>
		<dt_mechanics units="min">0.1</dt_mechanics>
		<dt_phenotype units="min">2</dt_phenotype>	
	</overall>
	
	<parallel>
		<omp_num_threads>4</omp_num_threads>
	</parallel> 
	
	<save>
		<folder>.</folder>  

		<full_data>
			<interval units="min">30.0</interval>
			<enable>True</enable>
		</full_data>
		
		<SVG>
			<interval units="min">30.0</interval>
			<enable>True</enable>
		</SVG>
		
		<legacy_data>
			<enable>true</enable>
		</legacy_data>
	</save>
	
	<microenvironment_setup>
		<variable ID="0" name="oxygen" units="mmHg">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">100000.0</diffusion_coefficient>
				<decay_rate units="1/min">0.1</decay_rate> 
			</physical_parameter_set>
			<initial_condition units="mmHg">38.0</initial_condition>
			<Dirichlet_boundary_condition enabled="true" units="mmHg">38.0</Dirichlet_boundary_condition>
		</variable>

		<variable ID="1" name="ecm" units="dimensionless">
			<physical_parameter_set>
				<diffusion_coefficient units="micron^2/min">1e-85</diffusion_coefficient>
				<decay_rate units="1/min">0.0</decay_rate> 
			<initial_condition units="mmHg">0.0</initial_condition>
			<Dirichlet_boundary_condition enabled="true" units="mmHg">0.0</Dirichlet_boundary_condition>
			</physical_parameter_set>
		</variable>

		<options>
			<calculate_gradients>True</calculate_gradients>
			<track_internalized_substrates_in_each_agent>True</track_internalized_substrates_in_each_agent>
			 
			<initial_condition enabled="false" type="matlab">
				<filename>./config/initial.mat</filename>
			</initial_condition>
			 
			<dirichlet_nodes enabled="false" type="matlab">
				<filename>./config/dirichlet.mat</filename>
			</dirichlet_nodes>
		</options>
	</microenvironment_setup>	
	
	<cell_definitions>
		<cell_definition ID="0" name="default">
			<phenotype>
				<cycle code="0" name="advanced_Ki67_cycle_model">  
					
					<transition_rates units="1/min"> 
						
						<rate end_index="1" fixed_duration="true" start_index="0">0.016666667</rate>
						
						<rate end_index="2" fixed_duration="true" start_index="1">1e30</rate>
						
						<rate end_index="0" fixed_duration="true" start_index="2">0.006666667</rate>
					</transition_rates>
				</cycle>
				
				<death>  
					<model code="100" name="apoptosis"> 
						<death_rate units="1/min">5.31667e-05</death_rate>
						<transition_rates units="1/min">
							<rate end_index="1" fixed_duration="true" start_index="0">1e30</rate>
						</transition_rates>
					</model> 

					<model code="101" name="necrosis">
						<death_rate units="1/min">0.0</death_rate>
						<transition_rates units="1/min">
							<rate end_index="1" fixed_duration="false" start_index="0">9e9</rate>
							<rate end_index="2" fixed_duration="true" start_index="1">1.15741e-5</rate>
						</transition_rates>
					</model> 
				</death>					

				<motility>  
					<speed units="micron/min">1</speed>
					<persistence_time units="min">1</persistence_time>
					<migration_bias units="dimensionless">0</migration_bias>
					
					<options>
						<enabled>false</enabled>
						<use_2D>false</use_2D>
					</options>
				</motility>

				<intracellular type="maboss">
					<bnd_filename>../data/boolean_network/CCM_mod4_1.bnd</bnd_filename>
					<cfg_filename>../data/boolean_network/CCM_mod4_1.cfg</cfg_filename>
					<time_step units="min">12</time_step>
					<initial_values>
						<initial_value node="ECM_sensing" units="dimensionless">0.5</initial_value>
						<initial_value node="Apoptosis" units="dimensionless">0.01</initial_value>
					</initial_values>
					<mutations>
						<mutation node="Hypoxia" units="dimensionless">1.0</mutation>
					</mutations>
				</intracellular>			
			</phenotype>
			<custom_data />
		</cell_definition>
	</cell_definitions>	
	
	<user_parameters>
		<random_seed type="int" units="dimensionless">0</random_seed> 
		 
		<polarity_coefficient type="double" units="">0.5</polarity_coefficient>
		<persistence type="double" units="">0.5</persistence>
    	<motility_amplitude_min type="double" units="">0.1</motility_amplitude_min>
    	<motility_amplitude_max type="double" units="">0.5</motility_amplitude_max>
    	<mode_motility type="int" units="">1</mode_motility>
		<contact_cell_ECM_threshold type="double" units="">0.5</contact_cell_ECM_threshold>
		<contact_cell_cell_threshold type="double" units="">0.9</contact_cell_cell_threshold> 
		<homotypic_adhesion_min type="double" units="">0.2</homotypic_adhesion_min>
		<homotypic_adhesion_max type="double" units="">0.8</homotypic_adhesion_max>
		<heterotypic_adhesion_min type="double" units="">0.2</heterotypic_adhesion_min>
		<heterotypic_adhesion_max type="double" units="">0.8</heterotypic_adhesion_max>
		<cell_basement_membrane_repulsion type="double" units="">10.0</cell_basement_membrane_repulsion>
		<cell_basement_membrane_adhesion type="double" units="">1.70577155519015</cell_basement_membrane_adhesion>
		<ecm_adhesion_min type="int" units="">1</ecm_adhesion_min>
		<ecm_adhesion_max type="int" units="">2</ecm_adhesion_max>
		<cell_ecm_repulsion type="double" units="">10.0</cell_ecm_repulsion>
		<ecm_degradation type="int" units="">5</ecm_degradation>
		<max_interaction_factor type="double" units="">1.3</max_interaction_factor>
		<cell_radius type="double" units="">8.413</cell_radius>
		<nucleus_radius type="double" units="">5.052</nucleus_radius>
		<fluid_fraction type="double" units="">0.75</fluid_fraction>
		<solid_nuclear type="int" units="">135</solid_nuclear>
		<solid_cytoplasmic type="int" units="">486</solid_cytoplasmic>
		<protein_threshold type="int" units="">1</protein_threshold>
		<init_cells_filename type="string" units="">../data/init.txt</init_cells_filename>
		<init_ecm_filename type="string" units="">../data/ecm.txt</init_ecm_filename>
		<tgfb_threshold type="double" units="dimensionless">1.0</tgfb_threshold>
		<membrane_shape type="string" units="">sphere</membrane_shape>	
		<membrane_length type="double" units="">10.0</membrane_length>
		<node_to_visualize type="string" units="">Cell_cell</node_to_visualize> 
		<color_function type="int" units="">0</color_function> 
	</user_parameters>
	
	
</PhysiCell_settings>